<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.sss"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" android:name=".MyApplication"
        android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_second" android:name=".SecondActivity" />
        <activity android:label="@string/title_activity_sell" android:name=".Sell"></activity>
        <activity android:label="@string/title_activity_library" android:name=".Library"></activity>
        <activity android:label="@string/title_activity_ebook" android:name=".Ebook"></activity>
        <activity android:label="@string/title_activity_ebook_details"
            android:name=".EBookDetails"></activity>
        <activity android:label="@string/title_activity_ebook_download"
            android:name=".Ebook_download"></activity>
        <activity android:label="@string/title_activity_sellet_details"
            android:name=".SellerDetails"></activity>
        <activity android:label="@string/title_activity_search" android:name=".SearchActivity"
            android:parentActivityName=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <meta-data android:name="android.app.searchable" android:resource="@xml/searchable" />
        </activity>
        <activity android:label="@string/title_activity_welcome"
            android:name=".WelcomeActivity"></activity>
        <activity android:label="@string/title_activity_setings"
            android:name=".Settings"></activity>
        <activity android:label="@string/title_activity_profile"
            android:name=".ProfileActivity"></activity>
    </application>

</manifest>
